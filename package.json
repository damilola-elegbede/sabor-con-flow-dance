{
  "name": "sabor-con-flow-dance",
  "version": "1.0.0",
  "description": "Sabor con Flow - Professional Salsa and Bachata Dance Studio Website",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:webpack": "webpack serve --mode development",
    "build": "node build-scripts/build-all.js --prod",
    "build:dev": "node build-scripts/build-all.js --dev",
    "build:fast": "npm run build:css:prod && npm run build:js",
    "build:analyze": "node build-scripts/build-all.js --prod --analyze",
    "build:verbose": "node build-scripts/build-all.js --prod --verbose",
    "build:legacy": "npm run clean:dist && npm run build:assets && npm run build:css:prod && npm run build:js && npm run build:critical && npm run build:html && npm run build:performance",
    "build:css": "node build-scripts/build-css.js",
    "build:css:prod": "node build-scripts/build-css.js --env=production",
    "watch:css": "node build-scripts/build-css.js --watch",
    "build:js": "webpack --mode production",
    "build:js:dev": "webpack --mode development",
    "build:js:analyze": "ANALYZE=1 webpack --mode production",
    "build:html": "node scripts/optimize-html.js",
    "build:assets": "node build-scripts/optimize-assets.js",
    "build:assets:images": "node build-scripts/optimize-assets.js --type=images",
    "build:assets:fonts": "node build-scripts/optimize-assets.js --type=fonts",
    "build:assets:svg": "node build-scripts/optimize-assets.js --type=svg",
    "build:critical": "node build-scripts/critical-css.js",
    "build:performance": "node build-scripts/build-performance-monitor.js",
    "build:baseline": "node build-scripts/build-performance-monitor.js --baseline",
    "clean:build": "node build-scripts/clean.js",
    "clean:dist": "node build-scripts/clean.js --dist",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:lighthouse": "lighthouse http://localhost:8000 --output json --output-path ./lighthouse-report.json",
    "test:accessibility": "node accessibility-tests.js",
    "test:contrast": "node contrast-tests.js",
    "lighthouse:accessibility": "lighthouse http://localhost:8000 --only=accessibility --output=json --output-path=./reports/accessibility-lighthouse.json",
    "axe:test": "node -e \"require('./static/js/accessibility-testing.js')\"",
    "accessibility:audit": "npm run test:accessibility && npm run test:contrast",
    "lint": "eslint src/ build-scripts/ --ext .js,.jsx,.ts,.tsx --max-warnings 50",
    "lint:css": "stylelint \"**/*.css\"",
    "format": "prettier --write .",
    "analyze": "npm run build:js:analyze && webpack-bundle-analyzer stats.json",
    "analyze:bundle": "webpack-bundle-analyzer static/js/**/*.js",
    "perf:monitor": "node build-scripts/build-performance-monitor.js",
    "perf:report": "node build-scripts/build-performance-monitor.js --report",
    "perf:baseline": "node build-scripts/build-performance-monitor.js --baseline",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:fonts": "node scripts/optimize-fonts.js",
    "generate:manifest": "node scripts/generate-manifest.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "prefetch": "node scripts/generate-prefetch.js",
    "critical": "critical static/css/styles.css --base staticfiles/ --inline --minify",
    "pwa:update": "workbox generateSW workbox-config.js",
    "deploy": "npm run build && npm run perf:monitor && vercel --prod",
    "deploy:preview": "npm run build:fast && vercel",
    "deploy:perf": "npm run build && npm run perf:report && vercel --prod",
    "clean": "rm -rf staticfiles dist node_modules/.vite build-metrics.json reports/*.json reports/*.html",
    "clean:cache": "rm -rf node_modules/.cache webpack.cache.json .parcel-cache",
    "postinstall": "husky install",
    "prepare": "husky install",
    "setup:testing": "npm install && mkdir -p reports",
    "setup:performance": "npm run clean:cache && npm run build:baseline",
    "ci:build": "npm run clean && npm install && npm run build",
    "ci:test": "npm run test && npm run test:e2e",
    "ci:performance": "npm run perf:monitor",
    "ci:full": "npm run ci:build && npm run ci:test && npm run ci:performance"
  },
  "dependencies": {
    "@vercel/analytics": "^1.1.1",
    "@vercel/speed-insights": "^1.0.2",
    "intersection-observer": "^0.12.2",
    "lazysizes": "^5.3.2",
    "web-vitals": "^3.5.0",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-legacy": "^5.2.0",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.1.3",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.3",
    "babel-loader": "^9.1.3",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "compression-webpack-plugin": "^11.0.0",
    "critical": "^6.0.1",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.0",
    "html-webpack-plugin": "^5.6.0",
    "puppeteer": "^21.6.1",
    "husky": "^8.0.3",
    "lighthouse": "^11.4.0",
    "lint-staged": "^15.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "playwright": "^1.40.0",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.1",
    "purgecss": "^5.0.0",
    "sharp": "^0.33.1",
    "style-loader": "^3.3.3",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "vite": "^5.0.10",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.17.4",
    "vitest": "^1.1.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "workbox-cli": "^7.0.0",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md,html}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "salsa",
    "bachata",
    "dance",
    "studio",
    "lessons",
    "toronto",
    "latin",
    "dance-classes"
  ],
  "author": "Sabor con Flow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sabor-con-flow/dance-studio.git"
  },
  "homepage": "https://saborconflow.ca"
}
